pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World my  name is akshay'
            }
        }
        stage('mvn test') {
            steps {
                echo 'Hello World my  name is akshay'
            }
        }
        stage('mvn compile') {
            steps {
                echo 'Hello World my  name is akshay'
            }
        }
        stage('mvn verify') {
            steps {
                echo 'Hello World my  name is akshay'
            }
        }
    }
}





















